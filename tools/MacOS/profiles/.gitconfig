[pull]
	rebase = false
[core]
	quotepath = false
	pager = less -x1,5
[alias]
	st = status
	co = checkout
	ci = commit
[url "git@git.code.oa.com:"]
    insteadOf = https://git.code.oa.com/
	insteadOf = http://git.code.oa.com/
[url "git@git.woa.com:"]
    insteadOf = https://git.woa.com/
    insteadOf = http://git.woa.com/

[diff "bin2text"]
	textconv = textract
	binary = true
